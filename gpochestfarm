repeat task.wait() until game.Players and game.Players.LocalPlayer and game.Players.LocalPlayer.Character

local done = false
syn.queue_on_teleport(loadstring(game:HttpGet("https://raw.githubusercontent.com/Skrapisismyscript/Solo_Hub/main/gpochestfarm"))())
local ch = game.Players.LocalPlayer.Character
local hum = ch.Humanoid
hum.Parent = nil
local done = true
hum.Parent = ch
task.spawn(function()
    while task.wait() do
        pcall(function()
            for i,v in pairs (game:GetService("Workspace").Env:GetChildren()) do
                if v.Name == "Part" and v:FindFirstChild("ClickDetector") then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
                    task.wait(0.1)
                    fireclickdetector(v:FindFirstChild("ClickDetector"))
                end
            end
            if not game:GetService("Workspace").Env:FindFirstChild("Part") then
                local module = loadstring(game:HttpGet"https://raw.githubusercontent.com/LeoKholYt/roblox/main/lk_serverhop.lua")()
                module:Teleport(game.PlaceId)
            end
        end)
    end
end)

