task.spawn(function()
    repeat task.wait()
        pcall(function()
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(8)
game:GetService("ReplicatedStorage").Events.footstep:FireServer()

            local args = {
                [1] = 10,
                [2] = "dash"
            }
            
            game:GetService("ReplicatedStorage").Events.takestam:FireServer(unpack(args))
        end)
    until false
    
end)
